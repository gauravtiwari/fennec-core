{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
      "document": true,
      "window": true,
      "fetch": true,
      "FileReader": true,
      "$": true
    },
    "rules": {
       "jsx-a11y/href-no-hash": "off",
       "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
       "react/forbid-prop-types": [0, { "forbid": ["object"] }]
    }
}
